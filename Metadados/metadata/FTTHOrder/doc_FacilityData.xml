  <methodList>
    <method name="InstallEquipmentPermission" type="permMethod">
      <script><![CDATA[
        var cwOrderId = this.cwOrderId;
        var ord = Order.getOrderById(cwOrderId);
        var orderType = ord.OrderHeader.tipoOS;

        var actualTask = UserProfile.getUserTask();
        if((actualTask == "FTTHProcess.InstallRemoveEquipmentManualTaskInterface/InstallRemoveEquipmentTask") && (orderType == "TIPO_ORDEM_INSTALACAO")){
                return true;
        } else if ((actualTask == "FTTHUserInterface.ClickManualActivityInterface/BAOpenningOperation") && (orderType == "TIPO_ORDEM_INSTALACAO")){
                return true;
        }
            return false;
      ]]></script>
    </method>
    <method name="RemoveEquipmentPermission" type="permMethod">
      <script><![CDATA[
        var cwOrderId = this.cwOrderId;
        var ord = Order.getOrderById(cwOrderId);
        var orderType = ord.OrderHeader.tipoOS;


        var actualTask = UserProfile.getUserTask();
        if((actualTask == "FTTHProcess.InstallRemoveEquipmentManualTaskInterface/InstallRemoveEquipmentTask") && (orderType == "TIPO_ORDEM_RETIRADA")){
                return true;
        } else if((actualTask == "FTTHUserInterface.ClickManualActivityInterface/BAOpenningOperation") && (orderType == "TIPO_ORDEM_RETIRADA")){
                return true;
        }
            return false;
      ]]></script>
    </method>
    <method name="editableCDO" type="permMethod">
      <script><![CDATA[
        var actualTask = UserProfile.getUserTask();
        var editableTask = "FTTHUserInterface.InventoryHandleErrorInterface/AssignFacilityHandleError";
        var result = (actualTask == editableTask);
        FTTHCommon.log(this.cwOrderId, "000000", actualTask, editableTask, "Editable CDO", result);
        return result;
      ]]></script>
    </method>
  </methodList>
