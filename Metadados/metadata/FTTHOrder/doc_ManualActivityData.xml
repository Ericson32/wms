  <methodList>
    <method name="cwLeafInitAction$statusAtividade" type="laction">
      <path>doc_FTTHOrder.ManualActivityData/leaf_statusAtividade</path>
      <type>initializer</type>
      <valueType>doc_FTTHOrder.ManualActivityData/leaf_statusAtividade</valueType>
      <script><![CDATA[
        //0 - aberta, 1 - fechada
        document.statusAtividade = 0;
      ]]></script>
    </method>
    <method name="ManualActivityPermission" type="permMethod">
      <privilegesList>
        <privileges type="ppriv">
          <privilege>closeIssue</privilege>
        </privileges>
      </privilegesList>
      <script><![CDATA[
        var actualTask = UserProfile.getUserTask();
        if(actualTask != null){
            var task = actualTask.split(".");
            var operation = this.operationWorklist.split(".");
            if(task[1] == operation[1] && this.statusAtividade == 0){
                    return true;
            }else if (actualTask == "FTTHUserInterface.ClickManualActivityInterface/BAOpenningOperation"
                ||    actualTask == "FTTHUserInterface.ClickManualActivityInterface/BAJumperOpeningOperation"
                ||    actualTask == "FTTHUserInterface.SGFTManualActivityInterface/SGFTBAOpenning"){
                return true;
            }
        }
            return false;
      ]]></script>
    </method>
    <method name="technicianData" type="permMethod">
      <script><![CDATA[
        if(this.statusAtividade == 0){
            var userId = Global.getUserId();
            var userDoc = UserProfile.getUserProfile(userId);
            var matriculaTecnico = Global.trim(userDoc.profileId);
            var nomeTecnico = Global.trim(userDoc.userName);
            this.matriculaTecnico = matriculaTecnico;
            this.nomeTecnico = nomeTecnico;
            //this.dataFechamento = new Date(); Correção defeito PRJ14008 #66
        }
        return false;
      ]]></script>
    </method>
    <method name="cwOnDocLoad" type="action">
      <system>true</system>
      <script><![CDATA[
        this.cw$super_cwOnDocLoad();
        /*
        * Foi criada uma variável transiente com o nome de "cwDocIDPSR" no document manual, pois ao criar o selectbox ele está exibindo o cwdocid no value do option.
        * Essa ao selecionar uma opção na selectbox esse script é chamado e seta os valores de nomePSR e codigoPSR que são atributos persistentes.
        */
        var vNomePSR = this.nomePSR ? this.nomePSR.trim() : '';
        if(vNomePSR == '') return;

        var searchDoc = new Document("FTTHReportApplication:PSR");
        searchDoc.nomePSR = this.nomePSR;
        var result = Finder.runSearch("FTTHOrder:codigoPSRFinder", searchDoc);
        this.cwDocIDPSR =  result[0].cwDocId;
      ]]></script>
    </method>
  </methodList>
