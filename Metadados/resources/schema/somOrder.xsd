	<!-- Documents -->

	<complexType name="Attribute">
		<annotation>
			<documentation>&lt;p style=&#34;margin-top: 0&#34;&gt;      Document com informacoes sobre o item pedido.    &lt;/p&gt;</documentation>
			<appinfo>
				<label>Attribute</label>
			</appinfo>
		</annotation>
			<complexContent>
				<extension base="cwf_system:Document">
					<sequence>
						<element name="cwDocId" type="cwf:cwDocId"/>
						<element name="cwDocStamp" type="cwf:cwDocStamp"/>
						<element name="cwOrderCreationDate" type="cwf:cwOrderCreationDate"/>
						<element name="cwOrderId" type="cwf:cwOrderId"/>
						<element name="cwParentId" type="cwf:cwParentId"/>
						<element name="lastUpdatedDate" type="cwf:lastUpdatedDate"/>
						<element name="updatedBy" type="cwf:updatedBy"/>
						<element name="name" type="somOrder:attributeName"/>
						<element name="value" type="somOrder:attributeValue"/>
						<element name="action" type="somOrder:attributeAction"/>
						<element name="type" type="somOrder:attributeType"/>
						<element name="valuePrevious" type="somOrder:attributeValuePrevious"/>
					</sequence>
						</extension>
					</complexContent>
				</complexType>

	<complexType name="Header">
		<annotation>
			<documentation>&lt;p style=&#34;margin-top: 0&#34;&gt;      Document com informacoes sobre a compra do cliente.    &lt;/p&gt;</documentation>
			<appinfo>
				<label>Header</label>
			</appinfo>
		</annotation>
			<complexContent>
				<extension base="cwf_system:Document">
					<sequence>
						<element name="cwDocId" type="cwf:cwDocId"/>
						<element name="cwDocStamp" type="cwf:cwDocStamp"/>
						<element name="cwOrderCreationDate" type="cwf:cwOrderCreationDate"/>
						<element name="cwOrderId" type="cwf:cwOrderId"/>
						<element name="cwParentId" type="cwf:cwParentId"/>
						<element name="lastUpdatedDate" type="cwf:lastUpdatedDate"/>
						<element name="updatedBy" type="cwf:updatedBy"/>
						<element name="transactionDate" type="somOrder:transactionDate"/>
						<element name="eventID" type="somOrder:eventID"/>
						<element name="orderID" type="somOrder:orderID"/>
						<element name="version" type="somOrder:version"/>
						<element name="Timestamp" type="xsd:dateTime"/>
						<element name="previousOrderId" type="somOrder:orderID"/>
						<element name="nextOrderId" type="somOrder:orderID"/>
						<element name="cwOrderEndDate" type="cwt_pcdd:endDate"/>
						<element name="restartProcess" type="xsd:boolean"/>
					</sequence>
						</extension>
					</complexContent>
				</complexType>

